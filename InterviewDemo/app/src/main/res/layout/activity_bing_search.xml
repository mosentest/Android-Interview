<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".bingsearh.BingSearchActivity">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:visibility="visible"
        tools:visibility="visible">

        <EditText
            android:id="@+id/etKey"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:hint="请输入关键字"
            android:text="来了老弟" />


        <Button
            android:id="@+id/btnSearch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="再次搜索" />

    </LinearLayout>


    <TextView
        android:id="@+id/webViewUrl"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:padding="10dp"
        android:textSize="14sp" />

    <Button
        android:id="@+id/btnShouldInterceptRequest"
        android:layout_width="match_parent"
        android:text="shouldInterceptRequest"
        android:layout_height="wrap_content" />


    <moziqi.interviewdemo.webview.TouchWebView
        android:id="@+id/touchWebView"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </moziqi.interviewdemo.webview.TouchWebView>

</LinearLayout>